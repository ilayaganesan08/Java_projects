import java.util.*;
public class program1 {

	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		System.out.println("Enter the Number:");
		int n=s.nextInt();
		char ch1='A';
		int a=ch1;
		for (int i=1; i<=n; i++) 
		{
			for(int j=1; j<=i; j++)
			{
				System.out.print(ch1);
				
			}
		ch1++;
		System.out.println();
		}
		System.out.println();
	}

}